{
  "name": "tilez-vega-lite",
  "version": "0.0.3",
  "author": "Torsten Sprenger <mail@spren9er.de>",
  "description": "Vega-Lite for Svelte layout engine tilez",
  "keywords": [
    "svelte",
    "vega-lite",
    "svg",
    "chart",
    "data visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spren9er/tilez-vega-lite.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spren9er/tilez-vega-lite/issues"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "peerDependencies": {
    "svelte": "^3.55.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.8.3",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "apache-arrow": "^11.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte-check": "^3.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "tilez": "^0.0.32",
    "vega": "^5.22.1",
    "vega-embed": "^6.21.3",
    "vega-lite": "^5.6.1"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
